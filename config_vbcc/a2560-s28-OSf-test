-cc=vbccm68k -cpu=68040 -quiet %s -o= %s %s -O=%ld -Iconfig_vbcc/include
-ccv=vbccm68k %s -cpu=68040 -o= %s %s -O=%ld -Iconfig_vbcc/include
-as=vasmm68k_mot -quiet -Fvobj -nowarn=62 %s -o %s
-asv=vasmm68k_mot -Fvobj -nowarn=62 %s -o %s
-rm=rm -f %s
-rmv=rm %s
-ld=vlink -bsrec28 -x -Cvbcc config_vbcc/minimal_startup.o config_vbcc/lib/syscalls.o config_vbcc/lib/io.o %s %s -Lconfig_vbcc/lib -lm -la2560_sys -Tconfig_vbcc/osf_vlink.cmd -lvc -o %s -Mmapfile
-l2=vlink -bsrec28 -x  -Cvbcc  %s %s -Lconfig_vbcc/lib -lm -la2560_sys -Tconfig_vbcc/osf_vlink.cmd -o %s -Mmapfile
-ldv=vlink -bsrec28 -t -x -Cvbcc lib/startup.o lib/syscalls.o lib/io.o %s %s -Lconfig_vbcc/lib -lm -la2560_sys -Tconfig_vbcc/osf_vlink.cmd -lvc -o %s -Mmapfile
-l2v=vlink -bsrec28 -t -x -Cvbcc %s %s -Lconfig_vbcc/lib -lm -la2560_sys -Tconfig_vbcc/osf_vlink.cmd -o %s -Mmapfile
-ul=-l%s
-cf=-F%s
-ml=1000
